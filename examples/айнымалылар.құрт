функция ештеңе негізгі() {
    айнымалы аты жол = "Асылбек";
    айнымалы жасы бүтін = 25;
    айнымалы салмағы бөлшек = 75.5;
    айнымалы студентПе шын = иә;
    
    жаз("=== ЖЕКЕ МӘЛІМЕТ ===");
    жаз("Аты: ");
    жаз(аты);
    жаз("Жасы: ");
    жаз(жасы);
    жаз("Салмағы: ");
    жаз(салмағы);
    жаз("Студент: ");
    егер(студентПе == иә) {
        жаз("Иә");
    } әйтпесе {
        жаз("Жоқ");
    }
}
