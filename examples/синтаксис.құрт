функция ештеңе негізгі() {
    сәлем();
    құрылымМысал();
}

функция ештеңе сәлем() {
    жаз("Сәлем, әлем", 150, 53.52, иә, жоқ);
    жаз(1+1-3*4/2);
    жаз(!иә);
    жаз("Сәлем, әлем"=="Сәлем, әлем");
    жаз(10<=8);
    жаз(көбейту(90, 10));
    айнымалы нүктелер [6]бүтін;
    жаз(нүктелер);
    айнымалы нүктелер1 [3]бүтін = {1,3,5};
    нүктелер1[0] = 5;
    жаз(нүктелер1);
    нүктелер1 = {9,0,51};
    жаз(нүктелер1);
    жаз(-нүктелер1[2]);
}

функция бүтін көбейту(а бүтін, б бүтін) {
    айнымалы с бүтін = а * б;
    қайтар с;
}

құрылым нүкте {
    х бүтін,
    у бүтін,
}

функция ештеңе құрылымМысал() {
    айнымалы а нүкте;
    жаз("А нүктесі: ", а.х, а.у);

    нүктеӨзгерту(а);
    жаз("Өзгертілген А нүктесі: ", а.х, а.у);

    жаз("X * Y = ", нүктеКөбейту(а));
}

функция ештеңе нүктеӨзгерту(н нүкте) {
    н.х = 5;
    н.у = 10;
}

функция бүтін нүктеКөбейту(н нүкте) {
    қайтар н.х * н.у;
}
