айнымалы бөлу2 бөлшек = температура / 2.0;

айнымалы тең1 шын = жасы == 25;
айнымалы тең2 шын = температура == 36.6;
айнымалы теңЕмес1 шын = жасы != 30;

айнымалы кіші1 шын = жасы < 30;
айнымалы үлкен1 шын = жасы > 18;
айнымалы кішіТең1 шын = жасы <= 25;
айнымалы үлкенТең1 шын = жасы >= 21;

айнымалы және1 шын = жасы > 18 && температура < 40.0;
айнымалы және2 шын = шындық && !жалған;
айнымалы немесе1 шын = жасы < 18 || жасы > 65;

айнымалы күрделіӨрнек1 бүтін = жасы + 5 * 2;
айнымалы күрделіӨрнек2 бүтін = жасы * 2 + 10;
айнымалы күрделіӨрнек3 бөлшек = температура * 1.5 - 2.0;

айнымалы массивӨрнектері1 бүтін = жыл[0] + жыл[1];
айнымалы массивӨрнектері2 бүтін = жыл[0] * жыл[2] - жыл[1];

айнымалы унарлыӨрнектер1 бүтін = -жасы + 50;
айнымалы унарлыӨрнектер2 бөлшек = -температура * 2.0;

құрылым Нүкте {
    x бөлшек,
    y бөлшек,
}

құрылым Адам {
    аты жол,
    жасы бүтін,
    биіктігі бөлшек,
    белсенді шын,
}

құрылым Мекенжай {
    көше жол,
    үй бүтін,
    қала жол,
}

құрылым ТолықАдам {
    жеке Адам,
    мекенжайы Мекенжай,
    жалақысы бөлшек,
}

құрылым Компания {
    атауы жол,
    қызметкерСаны бүтін,
    орналасуы Мекенжай,
    белсенді шын,
}

