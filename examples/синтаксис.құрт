айнымалы структураӨрнектері2 бүтін = адам1.жасы * 2 + 5;
айнымалы структураӨрнектері3 шын = адам1.жасы >= 18 && адам1.белсенді;

айнымалы терісНүктеX бөлшек = -нүкте1.x;
айнымалы емесАдамБелсенді шын = !адам1.белсенді;

айнымалы массивИндекс бүтін = жыл[нүкте1.x];
айнымалы структураИндекс бөлшек = биіктік[адам1.жасы];

айнымалы аралас1 бүтін = жасы + жыл[0] - адам1.жасы * 2;
айнымалы аралас2 бөлшек = температура + нүкте1.x - биіктік[2] / 2.0;
айнымалы аралас3 шын = жасы >= адам1.жасы && температура > нүкте1.y || !жалған;

