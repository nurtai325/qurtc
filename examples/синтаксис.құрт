функция ештеңе негізгі() {
    сәлем();
}

функция ештеңе сәлем() {
    жаз("Сәлем, әлем", 150, 53.52, иә, жоқ);
    жаз(1+1-3*4/2);
    жаз(!иә);
    жаз("Сәлем, әлем"=="Сәлем, әлем");
    жаз(10<=8);
    жаз(көбейту(90, 10));
	айнымалы бірНүкте нүкте;
    жаз(нүктеКөбейту(бірНүкте));
}

функция бүтін көбейту(а бүтін, б бүтін) {
    айнымалы с бүтін = а * б;
    қайтар с;
}

құрылым нүкте {
    х бүтін,
    у бүтін,
}

функция бүтін нүктеКөбейту(а нүкте) {
    айнымалы с бүтін = а.х * б.х;
    қайтар с;
}
