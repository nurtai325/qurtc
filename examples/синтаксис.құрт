айнымалы жасы бүтін = 25;
айнымалы жыл [3]бүтін = {2025, 32, 13};
айнымалы температура бөлшек = 36.6;
айнымалы биіктік [4]бөлшек = {3.14, 42.2, 42.5, 63.2};
айнымалы аты жол = "Айгүл";
айнымалы шындық шын = иә;
айнымалы жалған шын = жоқ;

айнымалы қосынды бүтін = жасы + 15;
айнымалы көбейтінді бүтін = жасы * 2;
айнымалы салыстыру шын = жасы > 18;
айнымалы логикалық шын = жасы > 18 && температура < 40.0;

айнымалы алғашқыЖыл бүтін = жыл[0];
айнымалы екіншіЖыл бүтін = жыл[1];
айнымалы индекс бүтін = 1;
айнымалы таңдалғанЖыл бүтін = жыл[индекс];

құрылым Нүкте {
    x бөлшек,
    y бөлшек,
}

құрылым Адам {
    аты жол,
    жасы бүтін,
    биіктігі бөлшек,
    белсенді шын,
}

құрылым Ұйым {
    атауы жол,
    қызметкерСаны бүтін,
    орналасуы Нүкте,
}

айнымалы нүкте1 Нүкте;
айнымалы адам1 Адам;
айнымалы ұйым1 Ұйым;
айнымалы нүктелер [3]Нүкте;
айнымалы адамдар [5]Адам;

айнымалы нүктеX бөлшек = нүкте1.x;
айнымалы адамАты жол = адам1.аты;
айнымалы ұйымАтауы жол = ұйым1.атауы;
айнымалы орналасуX бөлшек = ұйым1.орналасуы.x;

функция бүтін көбейту(а бүтін, б бүтін) {
    айнымалы нәтиже бүтін;
    айнымалы уақытша бүтін = а;
    нәтиже = уақытша * б;
    айнымалы тексеру бүтін = қосу(нәтиже, 0);
    қайтар нәтиже;
}

функция бүтін қосу(бірінші бүтін, екінші бүтін) {
    айнымалы жиын бүтін;
    жиын = бірінші + екінші;
    қайтар жиын;
}

функция бөлшек азайту(а бөлшек, б бөлшек) {
    айнымалы алғашқы бөлшек = а;
    айнымалы екінші бөлшек = б;
    айнымалы айырма бөлшек;
    айырма = алғашқы - екінші;
    қайтар айырма;
}

функция шын салыстыр(сол бүтін, оң бүтін) {
    айнымалы нәтиже шын;
    айнымалы үлкен шын = сол > оң;
    айнымалы кіші шын = сол < оң;
    нәтиже = үлкен || кіші;
    қайтар нәтиже;
}

функция Нүкте нүктеЖасау(x бөлшек, y бөлшек) {
    айнымалы жаңа Нүкте;
    жаңа.x = x;
    жаңа.y = y;
    айнымалы тексеру бөлшек = азайту(x, y);
    қайтар жаңа;
}

функция бөлшек арақашықтық(нүкте1 Нүкте, нүкте2 Нүкте) {
    айнымалы deltaX бөлшек = нүкте2.x - нүкте1.x;
    айнымалы deltaY бөлшек = нүкте2.y - нүкте1.y;
    айнымалы квадратX бөлшек = deltaX * deltaX;
    айнымалы квадратY бөлшек = deltaY * deltaY;
    айнымалы қосынды бөлшек = азайту(квадратX, -квадратY);
    қайтар қосынды;
}

функция Адам адамЖасау(аты жол, жасы бүтін, биіктігі бөлшек) {
    айнымалы жаңаАдам Адам;
    жаңаАдам.аты = аты;
    жаңаАдам.жасы = жасы;
    жаңаАдам.биіктігі = биіктігі;
    айнымалы ересек шын = салыстыр(жасы, 17);
    жаңаАдам.белсенді = ересек;
    қайтар жаңаАдам;
}

функция ештеңе массивӨзгерт(сандар [3]бүтін, индекс бүтін, мән бүтін) {
    айнымалы уақытша бүтін = сандар[индекс];
    сандар[индекс] = мән;
    айнымалы жаңа бүтін = көбейту(мән, 2);
    сандар[0] = жаңа;
}

функция ештеңе нүктеӨзгерт(нүкте Нүкте, жаңаX бөлшек, жаңаY бөлшек) {
    айнымалы ескі бөлшек = нүкте.x;
    нүкте.x = жаңаX;
    нүкте.y = жаңаY;
    айнымалы есептелген бөлшек = азайту(жаңаX, ескі);
    нүкте.x = есептелген;
}

функция бүтін массивҚосындысы(сандар [4]бүтін) {
    айнымалы жиын бүтін = 0;
    айнымалы уақытша бүтін;
    уақытша = сандар[0];
    жиын = қосу(жиын, уақытша);
    уақытша = сандар[1];
    жиын = қосу(жиын, уақытша);
    уақытша = сандар[2];
    жиын = қосу(жиын, уақытша);
    қайтар жиын;
}

функция Ұйым ұйымЖасау(атауы жол, саны бүтін, орны Нүкте) {
    айнымалы жаңаҰйым Ұйым;
    жаңаҰйым.атауы = атауы;
    жаңаҰйым.қызметкерСаны = саны;
    жаңаҰйым.орналасуы = орны;
    айнымалы үлкенҰйым шын = салыстыр(саны, 50);
    қайтар жаңаҰйым;
}

функция ештеңе құрылымӨңдеу(адам Адам, жасыҚосу бүтін, биіктікҚосу бөлшек) {
    айнымалы ескіЖасы бүтін = адам.жасы;
    айнымалы жаңаЖасы бүтін = қосу(ескіЖасы, жасыҚосу);
    адам.жасы = жаңаЖасы;
    айнымалы ескіБиіктік бөлшек = адам.биіктігі;
    айнымалы жаңаБиіктік бөлшек = азайту(ескіБиіктік, -биіктікҚосу);
    адам.биіктігі = жаңаБиіктік;
}

функция шын күрделіТексеру(адам Адам, минЖас бүтін, максБиіктік бөлшек) {
    айнымалы жасТексеру шын = салыстыр(адам.жасы, минЖас);
    айнымалы биіктікТексеру шын = адам.биіктігі < максБиіктік;
    айнымалы белсендіТексеру шын = !адам.белсенді;
    айнымалы нәтиже шын = жасТексеру && биіктікТексеру;
    нәтиже = нәтиже || белсендіТексеру;
    қайтар нәтиже;
}

функция ештеңе массивӨңдеу(нүктелер [3]Нүкте, индекс бүтін) {
    айнымалы таңдалған Нүкте = нүктелер[индекс];
    айнымалы жаңаX бөлшек = азайту(таңдалған.x, 1.0);
    айнымалы жаңаY бөлшек = азайту(таңдалған.y, 2.0);
    нүктелер[индекс] = жаңаX;
    нүктелер[индекс] = жаңаY;
    айнымалы тексеру бөлшек = арақашықтық(таңдалған, нүктелер[0]);
}
